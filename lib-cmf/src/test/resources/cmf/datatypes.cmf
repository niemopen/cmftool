    <Name>Restrict1Type</Name>
    <Namespace structures:ref="test" xsi:nil="true"/>
    <DocumentationText>Exercise minLength, maxLength, pattern, enumeration facets</DocumentationText>
    <RestrictionBase structures:ref="xs.token" xsi:nil="true"/>
    <Facet>
      <FacetCategoryCode>enumeration</FacetCategoryCode>
      <FacetValue>FOO</FacetValue>
      <DocumentationText>The FOO token</DocumentationText>
    </Facet>
    <Facet>
      <FacetCategoryCode>enumeration</FacetCategoryCode>
      <FacetValue>BAR</FacetValue>
    </Facet>
    <Facet>
      <FacetCategoryCode>maxLength</FacetCategoryCode>
      <FacetValue>4</FacetValue>
    </Facet>
    <Facet>
      <FacetCategoryCode>minLength</FacetCategoryCode>
      <FacetValue>3</FacetValue>
    </Facet>
    <Facet>
      <FacetCategoryCode>pattern</FacetCategoryCode>
      <FacetValue>[A-Z]{3}</FacetValue>
    </Facet>
  </Restriction>
