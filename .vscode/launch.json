    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CrashDriver message schema",
            "request": "launch",
            "mainClass": "org.mitre.niem.cmftool.CMFTool",
            "projectName": "app-cmftool",
            "args": "m2xmsg -o ${workspaceFolder}/../tests/CrashDriver-m2xmsg ${workspaceFolder}/lib-cmf/examples/CrashDriver/model.cmf",
            "classPaths": [
                "${workspaceFolder}/app-cmftool/build/install/cmftool/lib/*"
            ],
        },
        {
            "type": "java",
            "name": "augment.cmf message schema",
            "request": "launch",
            "mainClass": "org.mitre.niem.cmftool.CMFTool",
            "projectName": "app-cmftool",
            "args": "m2xmsg -o ${workspaceFolder}/../tests/augment-m2xmsg ${workspaceFolder}/lib-cmf/src/test/resources/cmf/augment.cmf",
            "classPaths": [
                "${workspaceFolder}/app-cmftool/build/install/cmftool/lib/*"
            ],
        }
    ]
}