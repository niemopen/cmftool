<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
  targetNamespace="https://docs.oasis-open.org/niemopen/proxy/niem-xs/6.0/#"
  xmlns:appinfo="https://docs.oasis-open.org/niemopen/appinfo/6.0/#"
  xmlns:ct="https://docs.oasis-open.org/niemopen/conformanceTargets/3.0/#"
  xmlns:xs-proxy="https://docs.oasis-open.org/niemopen/proxy/niem-xs/6.0/#"
  xmlns:structures="https://docs.oasis-open.org/niemopen/structures/6.0/#"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  ct:conformanceTargets="https://docs.oasis-open.org/niemopen/specification/naming-and-design-rules/3.0/#ReferenceSchemaDocument"
  version="1"
  xml:lang="en-US">
  <xs:annotation>
    <xs:documentation>Proxy types that carry dictionary metadata and have XML data type simple contents.</xs:documentation>
    <xs:appinfo>
      <appinfo:LocalTerm definition="An inclusive maximum value. To be valid, a value must be less than or equal to the value of xs:maxInclusive." term="maxInclusive"/>
    </xs:appinfo>
  </xs:annotation>
  <xs:import namespace="https://docs.oasis-open.org/niemopen/structures/6.0/#" schemaLocation="../utility/structures.xsd"/>
  <xs:complexType name="decimal">
    <xs:annotation>
      <xs:documentation>A data type for arbitrary precision decimal numbers.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:decimal">
        <xs:anyAttribute processContents="strict"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="string">
    <xs:annotation>
      <xs:documentation>A data type for character strings in XML.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:anyAttribute processContents="strict"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:schema>
