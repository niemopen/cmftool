<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Model xmlns:mm="http://reference.niem.gov/specification/metamodel/1.0/" xmlns:structures="http://release.niem.gov/niem/structures/5.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://reference.niem.gov/specification/metamodel/1.0/">
  <Namespace structures:uri="nc">
    <NamespaceURI>http://release.niem.gov/niem/niem-core/5.0/</NamespaceURI>
    <NamespacePrefixName>nc</NamespacePrefixName>
    <DefinitionText>NIEM Core.</DefinitionText>
  </Namespace>
  <Namespace structures:uri="xs">
    <NamespaceURI>http://www.w3.org/2001/XMLSchema</NamespaceURI>
    <NamespacePrefixName>xs</NamespacePrefixName>
  </Namespace>
  <Property structures:uri="nc:Date">
    <Name>Date</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A full date.</DefinitionText>
    <SubPropertyOf structures:uri="nc:DateRepresentation" xsi:nil="true"/>
    <Datatype structures:uri="xs:date" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:DateRange">
    <Name>DateRange</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A time period measured by a starting and ending point.</DefinitionText>
    <SubPropertyOf structures:uri="nc:DateRepresentation" xsi:nil="true"/>
    <Class structures:uri="nc:DateRangeType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:DateRepresentation">
    <Name>DateRepresentation</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data concept for a representation of a date.</DefinitionText>
    <AbstractIndicator>true</AbstractIndicator>
  </Property>
  <Property structures:uri="nc:DateTime">
    <Name>DateTime</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A full date and time.</DefinitionText>
    <SubPropertyOf structures:uri="nc:DateRepresentation" xsi:nil="true"/>
    <Datatype structures:uri="xs:dateTime" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:DocumentFiledDate">
    <Name>DocumentFiledDate</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A date a document was officially filed with an organization or agency.</DefinitionText>
    <Class structures:uri="nc:DateType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:EndDate">
    <Name>EndDate</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>An end date.</DefinitionText>
    <Class structures:uri="nc:DateType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:StartDate">
    <Name>StartDate</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A date on which something begins.</DefinitionText>
    <Class structures:uri="nc:DateType" xsi:nil="true"/>
  </Property>
  <Class structures:uri="nc:DateRangeType">
    <Name>DateRangeType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a range of dates.</DefinitionText>
    <HasProperty mm:maxOccursQuantity="1" mm:minOccursQuantity="1" structures:sequenceID="1">
      <Property structures:uri="nc:StartDate" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:maxOccursQuantity="1" mm:minOccursQuantity="1" structures:sequenceID="2">
      <Property structures:uri="nc:EndDate" xsi:nil="true"/>
    </HasProperty>
  </Class>
  <Class structures:uri="nc:DateType">
    <Name>DateType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a calendar date.</DefinitionText>
    <HasProperty mm:maxOccursQuantity="unbounded" mm:minOccursQuantity="0" structures:sequenceID="1">
      <Property structures:uri="nc:DateRepresentation" xsi:nil="true"/>
    </HasProperty>
  </Class>
  <Datatype structures:uri="xs:date">
    <Name>date</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
  <Datatype structures:uri="xs:dateTime">
    <Name>dateTime</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
  <Datatype structures:uri="xs:string">
    <Name>string</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
</Model>
