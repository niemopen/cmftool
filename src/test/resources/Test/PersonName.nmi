<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Model xmlns:mm="http://reference.niem.gov/specification/metamodel/1.0/" xmlns:structures="http://release.niem.gov/niem/structures/5.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://reference.niem.gov/specification/metamodel/1.0/">
  <Namespace structures:uri="nc">
    <NamespaceURI>http://release.niem.gov/niem/niem-core/5.0/</NamespaceURI>
    <NamespacePrefixName>nc</NamespacePrefixName>
    <DefinitionText>NIEM Core.</DefinitionText>
  </Namespace>
  <Namespace structures:uri="xs">
    <NamespaceURI>http://www.w3.org/2001/XMLSchema</NamespaceURI>
    <NamespacePrefixName>xs</NamespacePrefixName>
  </Namespace>
  <Property structures:uri="nc:PersonFullName">
    <Name>PersonFullName</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A complete name of a person.</DefinitionText>
    <Class structures:uri="nc:PersonNameTextType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:PersonGivenName">
    <Name>PersonGivenName</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A first name of a person.</DefinitionText>
    <Class structures:uri="nc:PersonNameTextType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:PersonMaidenName">
    <Name>PersonMaidenName</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>An original last name or surname of a person before changed by marriage.</DefinitionText>
    <Class structures:uri="nc:PersonNameTextType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:PersonMiddleName">
    <Name>PersonMiddleName</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A middle name of a person.</DefinitionText>
    <Class structures:uri="nc:PersonNameTextType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:PersonName">
    <Name>PersonName</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A combination of names and/or titles by which a person is known.</DefinitionText>
    <Class structures:uri="nc:PersonNameType" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:partialIndicator">
    <Name>partialIndicator</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <Datatype structures:uri="xs:boolean" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:personNameCommentText">
    <Name>personNameCommentText</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <Datatype structures:uri="xs:string" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:personNameInitialIndicator">
    <Name>personNameInitialIndicator</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <Datatype structures:uri="xs:boolean" xsi:nil="true"/>
  </Property>
  <Property structures:uri="nc:truncationIndicator">
    <Name>truncationIndicator</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <Datatype structures:uri="xs:boolean" xsi:nil="true"/>
  </Property>
  <Class structures:uri="nc:PersonNameTextType">
    <Name>PersonNameTextType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a name by which a person is known, referred, or addressed.</DefinitionText>
    <ExtensionOfClass structures:uri="nc:ProperNameTextType" xsi:nil="true"/>
    <HasProperty mm:minOccursQuantity="0">
      <Property structures:uri="nc:personNameInitialIndicator" xsi:nil="true"/>
    </HasProperty>
  </Class>
  <Class structures:uri="nc:PersonNameType">
    <Name>PersonNameType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a combination of names and/or titles by which a person is known.</DefinitionText>
    <HasProperty mm:maxOccursQuantity="unbounded" mm:minOccursQuantity="0" structures:sequenceID="1">
      <Property structures:uri="nc:PersonGivenName" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:maxOccursQuantity="unbounded" mm:minOccursQuantity="0" structures:sequenceID="2">
      <Property structures:uri="nc:PersonMiddleName" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:maxOccursQuantity="unbounded" mm:minOccursQuantity="0" structures:sequenceID="3">
      <Property structures:uri="nc:PersonMaidenName" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:maxOccursQuantity="unbounded" mm:minOccursQuantity="0" structures:sequenceID="4">
      <Property structures:uri="nc:PersonFullName" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:minOccursQuantity="0">
      <Property structures:uri="nc:personNameCommentText" xsi:nil="true"/>
    </HasProperty>
  </Class>
  <Class structures:uri="nc:ProperNameTextType">
    <Name>ProperNameTextType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a word or phrase by which a person or thing is known, referred, or addressed.</DefinitionText>
    <ExtensionOfClass structures:uri="nc:TextType" xsi:nil="true"/>
  </Class>
  <Class structures:uri="nc:TextType">
    <Name>TextType</Name>
    <Namespace structures:uri="nc" xsi:nil="true"/>
    <DefinitionText>A data type for a character string.</DefinitionText>
    <HasValue structures:uri="xs:string" xsi:nil="true"/>
    <HasProperty mm:minOccursQuantity="0">
      <Property structures:uri="nc:truncationIndicator" xsi:nil="true"/>
    </HasProperty>
    <HasProperty mm:minOccursQuantity="0">
      <Property structures:uri="nc:partialIndicator" xsi:nil="true"/>
    </HasProperty>
  </Class>
  <Datatype structures:uri="xs:boolean">
    <Name>boolean</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
  <Datatype structures:uri="xs:date">
    <Name>date</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
  <Datatype structures:uri="xs:dateTime">
    <Name>dateTime</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
  <Datatype structures:uri="xs:string">
    <Name>string</Name>
    <Namespace structures:uri="xs" xsi:nil="true"/>
  </Datatype>
</Model>
